<%= form_for @board , :url => secret_index_path(:new => params[:new],:hash => "asdas") do |f| %>

    <%= f.label :title, "Title" %>
    <%= f.text_field :title %><br>

    <%= f.label :descrption, "Description" %>
    <%= f.text_area :descrption %><br>

    <%= f.label :date, "Date" %>
    <%= f.date_select :date %><br>

    <%= f.submit "Create" %>
<% end %>